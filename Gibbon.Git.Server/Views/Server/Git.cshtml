@model ServerGitModel
@{
    Layout = "~/Views/Server/_ServerLayout.cshtml";
    ViewBag.Title = Localizer["Settings_Index_Title"];
}


<div class="pure-form pure-form-aligned detail">
    <fieldset>
        <div class="pure-control-group">
            @Html.LabelFor(m => m.IsGitAvailable)
            <span>@(Model.IsGitAvailable ? @Localizer["Yes"] : @Localizer["No"])</span>
        </div>
    </fieldset>
</div>
