@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Model == null)
{    
    @Html.Partial("~/Views/Shared/_ItemNotFound.cshtml") 
}
else
{  
    <text>
    <h2>@Resources.Repository_Detail_Title</h2>
    <ul id="repositoryMenu" class="clearfix">
        <li>@Html.ActionLink(Resources.Repository_Detail_Detail, "Detail", new { id = ViewBag.ID }, new { @class = "detail"})</li>
        <li>@Html.ActionLink(Resources.Repository_Layout_Browse, "Tree/", new { id = ViewBag.ID }, new { @class = "browser" })</li>
        <li>@Html.ActionLink(Resources.Repository_Layout_Commits, "Commits/", new { id = ViewBag.ID }, new { @class = "commits" })</li>
    </ul> 
    </text>
}
@RenderBody()
